<div class="contents-container" id="videos-container">
  <p>Look a video</p>
  <a id="edit-button" href="/videos/{{video._id}}/update">Edit this video</a>
  <form method='post' action="/videos/{{video._id}}/delete">
    <input type='submit' name='delete' id="delete-button" value="Delete this video">
  </form>
  <div class="video-card">
    <div class="video-title">
    {{video.title}}
    </div>
    <div class="video-description">
    {{video.description}}
    </div>
    <iframe class="video-url" src="{{video.url}}"></iframe>
  </div>
</div>
